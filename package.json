{
	"name": "@jackhuynh1995/vue-cool-lightbox",
	"description": "A fork of pretty Vue.js component to display an image gallery lightbox inspired by fancybox with VK Video support",
	"type": "module",
	"version": "1.3.0",
	"private": false,
	"license": "MIT",
	"author": "Lucas Pulliese <lucaspulliese10@gmail.com>",
	"repository": {
		"type": "git",
		"url": "https://github.com/morevm/vue-cool-lightbox"
	},
	"bugs": {
		"url": "https://github.com/morevm/vue-cool-lightbox/issues"
	},
	"keywords": [
		"vue",
		"vuejs",
		"lightbox",
		"gallery",
		"image gallery",
		"image lightbox"
	],
	"files": [
		"dist"
	],
	"exports": {
		".": {
			"import": "./dist/vue-cool-lightbox.esm.js",
			"require": "./dist/vue-cool-lightbox.cjs"
		},
		"./styles": "./dist/vue-cool-lightbox.min.css"
	},
	"main": "./dist/vue-cool-lightbox.cjs",
	"module": "./dist/vue-cool-lightbox.esm.js",
	"scripts": {
		"dev": "vite --config vite.config.dev.js",
		"build": "vite build --config vite.config.build.js",
		"lint:eslint": "eslint \"**/*.{js,ts,vue,yaml,yml,json,md,html}\"",
		"lint:eslint:fix": "eslint \"**/*.{js,ts,vue,yaml,yml,json,md,html}\" --fix",
		"lint": "yarn lint:eslint",
		"lint:fix": "yarn lint:eslint:fix",
		"release": "release-it --disable-metrics"
	},
	"dependencies": {
		"body-scroll-lock": "^3.1.5",
		"vue": "^3"
	},
	"devDependencies": {
		"@morev/eslint-config": "^34.0.0",
		"@release-it/conventional-changelog": "^8.0.2",
		"@vitejs/plugin-vue": "^4.0.0",
		"eslint": "8.57.1",
		"release-it": "^17.6.0",
		"sass": "^1.79.2",
		"sass-loader": "^8.0.0",
		"terser": "^5.33.0",
		"typescript": "^5.6.2",
		"vite": "^5.4.7"
	}
}
